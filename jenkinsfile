              -Dsonar.login=sqp_b1dedb48744b794e538b2e0d37554c2b59fd0a2d"
   }
   
   stage("Maven Build") 
   {sh "echo maven verify" }
   stage("Maven Deploy") 
   {sh " echo Install tree "}
